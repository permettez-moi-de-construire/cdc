{
  "name": "@algar/cdc-webhooks",
  "version": "0.0.1",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/index",
    "start": "dotenv -e ../../.env -- ts-node src/index",
    "lint": "eslint src",
    "fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "generate": "dotenv -e ../../.env -- prisma generate",
    "reset": "dotenv -e ../../.env -- prisma migrate reset",
    "migrate": "dotenv -e ../../.env -- prisma migrate deploy",
    "migration:up": "dotenv -e ../../.env -- prisma migrate dev --create-only --name",
    "migration:down": "dotenv -e ../../.env -- prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-schema-datasource prisma/schema.prisma --script > down.sql"
  },
  "dependencies": {
    "@algar/cdc-amqp": "0.0.1",
    "@permettezmoideconstruire/amqp-connector": "^2.3.3",
    "@prisma/client": "^4.6.1",
    "axios": "^1.2.0",
    "chalk": "^4.1.2",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "short-uuid": "^4.2.2",
    "winston": "^3.8.2",
    "zod": "^3.19.1",
    "zod-express-middleware": "^1.4.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.0",
    "@types/express": "^4.17.14"
  }
}
