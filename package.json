{
  "name": "pg-amqp-poc",
  "version": "0.0.1",
  "main": "index.ts",
  "author": "Cyril CHAPON <cyril@permettezmoideconstruire.fr>",
  "license": "MIT",
  "scripts": {
    "dev:consume": "dotenv -- ts-node src/03-consume/index",
    "dev:log": "dotenv -- ts-node src/02-log/index",
    "dev:simulate": "dotenv -- ts-node src/01-simulate/index",
    "lint": "eslint src",
    "fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "generate": "prisma generate",
    "db:reset": "prisma migrate reset",
    "db:migrate": "prisma migrate deploy",
    "db:migration:up": "prisma migrate dev --create-only --name",
    "db:migration:down": "prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-schema-datasource prisma/schema.prisma --script > down.sql",
    "dbq:prepare": "yarn _dbq:create-repl && yarn _dbq:create-pub",
    "dbq:teardown": "yarn _dbq:drop-pub && yarn _dbq:drop-repl",
    "amqp:prepare": "yarn _amqp:create-vhost && yarn _amqp:create-exchange && yarn _amqp:create-queue && yarn _amqp:bind-queue",
    "amqp:teardown": "yarn _amqp:unbind-queue && yarn _amqp:drop-queue && yarn _amqp:drop-exchange && yarn _amqp:drop-vhost",
    "_dbq:create-repl": "dotenv -- ts-node src/_scripts/postgres/create-repl",
    "_dbq:drop-repl": "dotenv -- ts-node src/_scripts/postgres/drop-repl",
    "_dbq:create-pub": "dotenv -- ts-node src/_scripts/postgres/create-pub",
    "_dbq:drop-pub": "dotenv -- ts-node src/_scripts/postgres/drop-pub",
    "_amqp:create-vhost": "dotenv -- ts-node src/_scripts/amqp/create-amqp-vhost",
    "_amqp:drop-vhost": "dotenv -- ts-node src/_scripts/amqp/drop-amqp-vhost",
    "_amqp:create-exchange": "dotenv -- ts-node src/_scripts/amqp/create-amqp-exchange",
    "_amqp:drop-exchange": "dotenv -- ts-node src/_scripts/amqp/drop-amqp-exchange",
    "_amqp:create-queue": "dotenv -- ts-node src/_scripts/amqp/create-amqp-queue",
    "_amqp:drop-queue": "dotenv -- ts-node src/_scripts/amqp/drop-amqp-queue",
    "_amqp:bind-queue": "dotenv -- ts-node src/_scripts/amqp/bind-amqp-queue",
    "_amqp:unbind-queue": "dotenv -- ts-node src/_scripts/amqp/unbind-amqp-queue"
  },
  "dependencies": {
    "@permettezmoideconstruire/amqp-connector": "^2.3.3",
    "@prisma/client": "4.6.1",
    "amqplib": "^0.10.3",
    "axios": "^1.1.3",
    "envalid": "^7.3.1",
    "json-bigint": "^1.0.0",
    "pg-logical-replication": "^2.0.2",
    "prisma": "^4.6.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.0",
    "@types/json-bigint": "^1.0.1",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3"
  }
}
